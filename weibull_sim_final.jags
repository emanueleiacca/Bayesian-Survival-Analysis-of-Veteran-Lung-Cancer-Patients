
model {
  for (i in 1:N) {
    isCens[i] ~ dinterval(t[i], time[i])
    t[i] ~ dweib(shape, lambda[i])

    eta[i] <- inprod(beta[], X[i,])
    lambda[i] <- exp(-eta[i])
  }

  for (j in 1:K) {
    beta[j] ~ dnorm(0, 1)   # Moderately informative prior
  }

  shape ~ dgamma(2, 1)      # Prior with mean 2, variance 2
}
